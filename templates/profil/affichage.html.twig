    <div class="profil_div">
        <h1 class="titre_sortie">Profil</h1>
    </div>
        <br>
    <div class="profil_div div">
        <p class="gras">Pseudo : </p>&nbsp;
        <p>{{ profil.pseudo }}</p>
    </div>
    <div class="profil_div">
        <p class="gras">Nom : </p>&nbsp;
        <p>Nom : {{ profil.nom }}</p>
    </div>
    <div class="profil_div">
        <p class="gras">Pr√©nom : </p>&nbsp;
        <p>{{ profil.prenom }}</p>
