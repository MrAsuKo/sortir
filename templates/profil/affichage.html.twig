        <p>{{ profil.telephone }}</p>
    </div>
    <div class="profil_div">
        <p class="gras">Campus : </p>&nbsp;
        <p>{{ profil.campus.nom }}</p>
    </div>
    <div class="profil_div">
        <p class="gras">Mail : </p>&nbsp;
        <p>{{ profil.mail }}</p>
    </div>
        <div>
            <a class="btn btn-secondary bouton_creer" href="{{ path('app_accueil') }}">Retour</a>
        </div>
    </div>


{% endblock %}